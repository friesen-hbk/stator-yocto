#!/bin/sh
i2c_write -a 0x55 -r 132 0x0
i2c_write -a 0x55 -r 0 0x23
i2c_write -a 0x55 -r 5 0xdf
i2c_write -a 0x55 -r 6 0x33
i2c_write -a 0x55 -r 7 0x44
i2c_write -a 0x55 -r 8 0x2a
i2c_write -a 0x55 -r 9 0x08
i2c_write -a 0x55 -r 10 0x66
i2c_write -a 0x55 -r 11 0x00
i2c_write -a 0x55 -r 132 0x1
msleep 10
i2c_write -a 0x55 -r 132 0x4
